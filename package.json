{
  "name": "rss-reader",
  "version": "1.0.0",
  "private": true,
  "description": "RSS Reader Monorepo",
  "scripts": {
    "dev:web": "pnpm --filter rss-reader-web dev",
    "dev:server": "pnpm --filter rss-reader-server dev",
    "build": "pnpm -r build",
    "build:shared": "pnpm --filter @rss-reader/shared build",
    "build:web": "pnpm --filter rss-reader-web build",
    "build:server": "pnpm --filter rss-reader-server build"
  },
  "devDependencies": {
    "typescript": "^5.3.0"
  }
}
